<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.canvas.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane alignment="center" hgap="10" prefHeight="800.0" prefWidth="1200.0" vgap="10" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
   <children>
      <AnchorPane prefHeight="800.0" prefWidth="1200.0">
         <children>
            <Canvas fx:id="canvas" height="599.0" layoutX="200.0" layoutY="101.0" width="800.0" />
            <Label layoutX="907.0" layoutY="68.0" text="Speed:" />
            <Label layoutX="907.0" layoutY="41.0" text="Altitude:" />
            <Label fx:id="speed" layoutX="985.0" layoutY="68.0" text="INFO" />
            <Label fx:id="altitude" layoutX="985.0" layoutY="41.0" text="INFO" />
            <Button fx:id="beginButton" layoutX="575.0" layoutY="710.0" mnemonicParsing="false" onAction="#begin" text="BEGIN" />
            <Label layoutX="554.0" layoutY="68.0" text="Flying:" />
            <Label fx:id="flying" layoutX="618.0" layoutY="68.0" text="INFO" />
            <TableView fx:id="tableViewLog" layoutX="14.0" layoutY="221.0" prefHeight="200.0" prefWidth="200.0">
              <columns>
                <TableColumn prefWidth="75.0" text="C1" />
                <TableColumn prefWidth="75.0" text="C2" />
              </columns>
            </TableView>
            <Button fx:id="udpbutton" layoutX="14.0" layoutY="576.0" mnemonicParsing="false" onAction="#sendUdpMessage" text="Button" />
            <TextField fx:id="testmessagebox" layoutX="14.0" layoutY="532.0" />
         </children>
      </AnchorPane>
   </children>
</GridPane>
